{
  "name": "tree-sitter-sql-bigquery",
  "version": "0.0.4",
  "description": "SQL grammar fom tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter generate && tree-sitter test",
    "gen": "tree-sitter generate",
    "hl": "tree-sitter highlight",
    "parse": "tree-sitter parse",
    "prettier": "prettier --write .",
    "build-wasm": "tree-sitter build-wasm",
    "playground": "rm -f tree-sitter-sql-bigquery.wasm && npm run build-wasm && tree-sitter playground"
  },
  "keywords": [
    "parser",
    "lexer",
    "sql",
    "bigquery"
  ],
  "repository": "https://github.com/TKNGUE/tree-sitter-sql-bigquery",
  "author": "TKNGUE <takegue@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.2"
  },
  "devDependencies": {
    "graphviz": "^0.0.9",
    "prettier": "^2.4.1",
    "tree-sitter-cli": "^0.20.6"
  },
  "tree-sitter": [
    {
      "scope": "source.sql",
      "file-types": [
        "sql"
      ]
    }
  ]
}
